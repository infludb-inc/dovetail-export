{
  "name": "DVTLTSEX",
  "description": "Dovetail Transcript Export To Fibery",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "activeTab",
    "tabs",
    "scripting",
    "storage"
  ],
  "host_permissions": ["https://*.fibery.io/*"],
  "action": {
    "default_title": "Dovetail Export",
    "default_icon": "assets/icon-48.png",
    "default_popup": "popup/popup.html"
  },
  "icons": {
    "16": "assets/icon-16.png",
    "32": "assets/icon-32.png",
    "48": "assets/icon-48.png",
    "128": "assets/icon-128.png"
  }
}
